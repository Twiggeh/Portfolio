	"scripts": {
		"lint": "eslint ./",
		"debug": "echo NODE_ENV=development > .env && echo BACKEND_URL=http://localhost:5050 >> .env && webpack-dev-server --config ./config/webpack.dev.js --mode development --host 0.0.0.0 --port 5000",
		"build": "echo NODE_ENV=production > .env && echo BACKEND_URL=https://www.twiggeh.xyz >> .env && webpack --config ./config/webpack.prod.js --mode production",
		"buildalyze": "echo NODE_ENV=production > .env && echo ANALYZE=true >> .env && > .env && echo BACKEND_URL=http://localhost:5050 && webpack --config ./config/webpack.prod.js --mode production",
		"start": "echo NODE_ENV=development > .env &&  webpack-dev-server --config ./config/webpack.dev.js --mode development --open"
	},
